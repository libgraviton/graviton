{
  "collection": "App",
  "route": "app2",
  "type": "object",
  "aggregate": {
    "$match": {
      "order": {
        "$gte": 2
      }
    },
    "$group": {
      "_id": "app-count-2",
      "count": {
        "$sum": 1
      }
    }
  },
  "schema": {
    "title": "Application usage Above 2",
    "description": "Data use for application access with order gte 2",
    "type": "object",
    "representation": "pie",
    "properties": {
      "id": {
        "title": "ID",
        "description": "Unique identifier",
        "type": "string"
      },
      "count": {
        "title": "count",
        "description": "Sum of result",
        "type": "integer"
      }
    }
  }
}