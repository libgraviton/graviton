        </service>

        <service id="graviton.proxy.service.source.vontobel" class="Graviton\ProxyBundle\Service\Source\Redirect">
            <tag name="graviton.proxy.source" alias="vontobel" />
        </service>

        <service id="graviton.proxy.service.source.zugerkb" class="Graviton\ProxyBundle\Service\Source\Redirect">
            <tag name="graviton.proxy.source" alias="zugerkb" />
        </service>
-->
    </services>
</container>
